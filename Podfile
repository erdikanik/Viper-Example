  use_frameworks!

 pod 'Alamofire', :git => 'https://github.com/Alamofire/Alamofire.git', :branch => 'alamofire5'
 pod 'INTULocationManager'
 pod 'Toaster'
 
  target 'WeatherTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'WeatherUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
